package part_1;

import java.util.Scanner;

public class Tutorial_16_17 {

	public static void main(String[] args) {
		Scanner s = new Scanner (System.in);
		_16_17_ object = new _16_17_("lol");
		object.saying();
		
		System.out.println("Enter a thing- ");
		String n = s.nextLine();
		object.setName(n);
		object.saying();

	}

}
