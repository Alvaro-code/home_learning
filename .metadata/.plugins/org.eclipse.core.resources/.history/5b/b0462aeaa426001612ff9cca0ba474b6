package part_1;

public class _16_17_ {
	private String thingName;
	
	public _16_17_(String name){
		thingName = name;
	}
	public void setName(String name){
		thingName = name;
	}
	public String getName(){
		return thingName;
	}
	public void saying(){
		System.out.printf("Your thing was %s", thingName);
	
	}
}
